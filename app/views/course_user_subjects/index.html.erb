<p id="notice"><%= notice %></p>

<h1>Course User Subjects</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_user_subjects.each do |course_user_subject| %>
      <tr>
        <td><%= link_to 'Show', course_user_subject %></td>
        <td><%= link_to 'Edit', edit_course_user_subject_path(course_user_subject) %></td>
        <td><%= link_to 'Destroy', course_user_subject, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

